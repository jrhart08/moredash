{
  "name": "moredash",
  "version": "0.0.3",
  "description": "A general-purpose utility library for when lodash isn't enough",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "run-s lint test:coverage clean",
    "clean": "rimraf lib",
    "build": "tsc --project ./tsconfig.prod.json",
    "test": "jest --verbose",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrhart08/moredash.git"
  },
  "keywords": [
    "utility"
  ],
  "author": "Joseph Hart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrhart08/moredash/issues"
  },
  "homepage": "https://github.com/jrhart08/moredash#readme",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lodash": "^7.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.0"
  }
